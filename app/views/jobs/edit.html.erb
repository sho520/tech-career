<!-- 会社概要 -->
        <section class="job-form__inner__left__company__text">会社概要</section>

        <div class="job-form__inner__left__company__field">
          <%= f.label :企業名,class: 'job-form__inner__left__company__field__label' %><span class="from-wrapper__inner__main__formarea__field__flag">必須</span><br>
          <%= f.text_field :name, autofocus: true, autocomplete: "name", placeholder: "株式会社div", class: "job-form__inner__left__company__field__input" %>
        </div>

        <div class="job-form__inner__left__company__field">
          <%= f.label :業種,class: 'job-form__inner__left__company__field__label' %><span class="from-wrapper__inner__main__formarea__field__flag">必須</span><br>
